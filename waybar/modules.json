  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ",
      "deactivated": "ÔÅ∞"
    }
  },
  "clock": {
    "interval": 1,
    "format": "{:L%a %d  %H:%M}",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format-alt": "{:L%A, %e. %B %Y  %H:%M:%S}"
  },
  "cpu": {
    "format": "{usage}% Ôãõ",
    "tooltip": false
  },
  "memory": {
    "format": "{}% ÔÉâ"
  },
  "temperature": {
    "critical-threshold": 80,
    "format": "{temperatureC}¬∞C {icon}",
    "format-icons": [
      "Ôù´",
      "Ôãâ",
      "Ôù©"
    ]
  },
  "backlight": {
    "format": "{percent}% {icon}",
    "format-icons": [
      "Óéç",
      "Óèì",
      "Óèë",
      "Óèè",
      "Óèé",
      "Óèç",
      "Óèä",
      "Óèà",
      "Óéõ"
    ]
  },
  "battery": {
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {icon} ",
    "format-full": "{capacity}% {icon} ",
    "format-charging": "{capacity}% Ôóß ",
    "format-plugged": "{capacity}% Ôá¶ ",
    "format-alt": "{time} {icon}",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ]
